#include "resource.h"

ICONFAMILY "command.bmp" "" "" "command.bmp"

BITMAPFAMILY ID 1 RSCTYPE "Icon"
BEGIN
  BITMAP "luaL.bmp" BPP 8 DENSITY  72 TRANSPARENT 255 255 255
  BITMAP "luaH.bmp" BPP 8 DENSITY 144 TRANSPARENT 255 255 255
END

FORM MainForm 0 0 160 160
USABLE
NOFRAME
MENUID 1001
BEGIN
  TITLE ""
END

FORM PrefsForm 2 2 156 110
USABLE
MODAL
SAVEBEHIND
DEFAULTBTNID cancelBtn
BEGIN
  TITLE "Preferences"
  LABEL "Font:" 3001 4 18 FONT 1
  LABEL "6x10" lbl6x10 PREVRIGHT+5 PREVTOP
  PUSHBUTTON "6x10"  sel6x10  PREVLEFT PREVTOP AUTO AUTO GROUP 1
  PUSHBUTTON "8x14"  sel8x14  PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 1
  PUSHBUTTON "8x16"  sel8x16  PREVRIGHT+1 PREVTOP PREVWIDTH PREVHEIGHT GROUP 1
  LABEL "Foreground:" 3002 4 PREVBOTTOM+6 FONT 1
  GADGET ID fgCtl 70 PREVTOP 20 PREVHEIGHT
  LABEL "Background:" 3003 4 PREVBOTTOM+6 FONT 1
  GADGET ID bgCtl 70 PREVTOP 20 PREVHEIGHT
  LABEL "Highlight:" 3004 4 PREVBOTTOM+6 FONT 1
  GADGET ID hlCtl 70 PREVTOP 20 PREVHEIGHT
  BUTTON "Ok" okBtn 4 PREVBOTTOM+8 AUTO AUTO
  BUTTON "Cancel" cancelBtn PREVRIGHT+5 PREVTOP AUTO AUTO
  BUTTON "Defaults" dflBtn PREVRIGHT+15 PREVTOP AUTO AUTO
END

FORM ID 11002 AT (2 2 156 156)
  MODAL
  SAVEBEHIND
  USABLE
  DEFAULTBTNID 11006
BEGIN
  TITLE "About ^1XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
  FORMBITMAP AT (31 23) BITMAP 10000 USABLE
  LABEL "^1XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX" ID 11001 AT (15 74) USABLE FONT 1
  LABEL "v. ^1XXXXXXXXXXXXXXX" ID 11002 AT (15 85) USABLE FONT 1
  BUTTON "OK" ID 11006 AT (60 139 37 12) USABLE LEFTANCHOR FRAME
END

MENU 1001
BEGIN
  PULLDOWN "Options"
  BEGIN
    MENUITEM "About"       aboutCmd "A"
    MENUITEM "Preferences" prefsCmd "P"
    MENUITEM "Fork"        forkCmd  "F"
  END
  PULLDOWN "Edit"
  BEGIN
    MENUITEM "Paste" pasteCmd "P"
    MENUITEM "Copy"  copyCmd  "C"
  END
END

MENU frmScrpMenu
BEGIN
  PULLDOWN "Options"
  BEGIN
    MENUITEM "About" 1 "A"
  END
END

DATA "LuaS" ID 1 "command.lua"
DATA "LuaS" ID frmScrpID "form.lua"

DATA "JsSc" ID 1 "command.js"

HEX "wind" ID 1
  0x02 0x80 0x01 0x5E

VERSION "1.0"
